{
	"name": "angular-onboard",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"fmt": "oxfmt",
		"fmt-ci": "oxfmt --check",
		"lint": "oxlint --type-aware --type-check",
		"fix": "oxlint --fix",
		"openapi:generate": "openapi-generator-cli generate -i ./src/assets/openapi/openapi-onboard-registration.yaml -g typescript-angular -o ./src/app/api --additional-properties=npmName=@author/onboard-api,npmVersion=0.0.0,modelPropertyNaming=original,useSingleRequestParameter=true"
	},
	"dependencies": {
		"@angular/common": "^21.1.0",
		"@angular/compiler": "^21.1.0",
		"@angular/core": "^21.1.0",
		"@angular/forms": "^21.1.0",
		"@angular/platform-browser": "^21.1.0",
		"@angular/router": "^21.1.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@angular/build": "^21.1.3",
		"@angular/cli": "^21.1.3",
		"@angular/compiler-cli": "^21.1.0",
		"@openapitools/openapi-generator-cli": "^2.28.2",
		"@tailwindcss/postcss": "^4.1.12",
		"jsdom": "^27.1.0",
		"oxfmt": "catalog:voidzero",
		"oxlint": "catalog:voidzero",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.1.12",
		"typescript": "~5.9.2",
		"vitest": "^4.0.8"
	},
	"prettier": {
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			}
		],
		"printWidth": 100,
		"singleQuote": true
	},
	"packageManager": "pnpm@10.16.1"
}
